{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["jest"],
    "rules": {
        "no-use-before-define": ["error", {
            "functions": true,
            "classes": true,
            "variables": true,
            "allowNamedExports": false
        }],
        "no-console": "off"
    }
}
